#Github
Host m-coding.github.com
    IdentityFile ~/.ssh/github_m-coding

Host kermit.github.com
    IdentityFile ~/.ssh/github_kermit

Host fozzie.github.com
    IdentityFile ~/.ssh/github_fozzie

Host *github.com
    HostName github.com

#GitLab
Host misspiggy.gitlab.com
    IdentityFile ~/.ssh/gitlab_misspiggy

Host gonzo.gitlab.com
    IdentityFile ~/.ssh/gitlab_gonzo

Host *gitlab.com
    HostName gitlab.com

#Bitbucket
Host pepe.bitbucket.org
    HostName bitbucket.org
    IdentityFile ~/.ssh/bitbucket_pepe

#Common to all hosts
Host *
    User git
    PreferredAuthentications publickey
    #IdentitiesOnly yes
    #LogLevel DEBUG3